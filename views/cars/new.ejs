<%- include('../partials/header') %>

<link rel="stylesheet" href="/stylesheets/new.css">

<h2>Enter a New Toyota</h2>

<img src="https://cdn.pixabay.com/photo/2016/08/15/18/19/toyota-1596082_1280.png" class="cars">

<form id="add-review"
 action="/cars" method="POST">
  <label>Make:
    <select name="make">
      <option value="Toyota">Toyota</option>
    </select>
  </label><br>
  <label>Model:
    <select name="model">
      <option value="Corolla" selected>Corolla</option>
      <option value="Camary">Camary</option>
      <option value="Rav4">Rav4</option>
      <option value="Highlander">Highlander</option>
    </select>
  </label><br>
  <label>Year:
    <input type="Number" name="year">
  </label><br>
  <label>Availability:
    <input type="datetime-local" name="availability">
  </label><br>
  <input type="submit" value="submit">
</form>

<%- include('../partials/footer') %>
